{
  "name": "chat-app",
  "version": "1.2.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.5.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "emoji-picker-react": "^2.0.4",
    "express": "^4.16.3",
    "html-parse-stringify": "^1.0.2",
    "lodash": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "mongodb": "^3.1.4",
    "nexmo": "^2.3.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-icons": "^2.2.5",
    "react-images-upload": "^1.2.2",
    "react-intl": "^2.4.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.0.11",
    "socket.io": "^2.0.3",
    "twilio": "^2.11.1",
    "uuid": "^3.1.0",
    "watch": "^1.0.2"
  },
  "scripts": {
    "start": "concurrently 'npm run react' 'npm run server' 'npm run styles'",
    "react": "react-scripts start",
    "server": "nodemon src/server/index.js",
    "build": "react-scripts build",
    "styles": "node-sass --output-style compressed --include-path scss src/styles/index.scss src/styles/index.css",
    "styles:w": "nodemon -e scss -x \"npm run styles\"",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "npm run build",
    "watch": "npm run styles"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.11.0"
  }
}
